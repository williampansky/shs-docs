{
    "plugins": [
        "node_modules/jsdoc-vue-component",
        "plugins/markdown"
    ],
    "jsdoc-vue-component": {
        "log": false
    },
    "markdown": {
        "tags": [
            "author", 
            "classdesc", 
            "description", 
            "param", 
            "property", 
            "returns", 
            "see", 
            "throws", 
            "vue"
        ]
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc", 
            "closure"
        ]
    },
    "recurseDepth": 10,
    "source": {
        "include": [
            "README.md",
            "example/"
        ],
        "exclude": [
            "src/static/js/bundle.js",
            "src/static/js/clipboard.min.js",
            "src/static/js/linenumber.js",
            "src/static/js/highlight.pack.js",
            "src/static/js/prism-treeview.js",
            "src/static/js/prism.custom.js",
            "src/static/js/prism.min.js",
            "node_modules"
        ],
        "includePattern": ".+\\.(js|vue)$",
        "excludePattern": "(node_modules/|dist|.min)"
    },
    "opts": {
        "template": "src",
        "encoding": "utf8",
        "destination": "example/dist",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "tutorials": "example/dist/tutorials"
}