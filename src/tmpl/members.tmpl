<?js
    var data = obj;
    var self = this;
?>
<section id="<?js= id ?>" class="jsdoc-item uk-card uk-card-default uk-card-small" data-file="members.tmpl">
    <h4 class="jsdoc-item-name uk-card-header uk-card-title uk-background-gradient">
        <?js= data.attribs + name + (data.signature ? data.signature : '') ?>
        <span class="uk-label uk-light">Member</span>
    </h4>

    <div class="uk-card-body">
        <?js= this.partial('details.tmpl', data) ?>

        <?js if (data.description) { ?>
            <div class="description">
                <?js= data.description ?>
            </div>
        <?js } ?>

        <?js if (data.type && data.type.names) { ?>
            <h5>Type:</h5>
            <ul>
                <li><?js= self.partial('type.tmpl', data.type.names) ?></li>
            </ul>
        <?js } ?>

        <?js if (data.fires && fires.length) { ?>
            <h5>Fires:</h5>
            <ul>
                <?js fires.forEach(function(f) { ?>
                    <li><?js= self.linkto(f) ?></li>
                <?js }); ?>
            </ul>
        <?js } ?>

        <?js if (data.examples && examples.length) { ?>
            <h5 class="uk-margin-small-bottom">Example<?js= examples.length > 1? 's':'' ?></h5>
            <?js= this.partial('examples.tmpl', examples) ?>
        <?js } ?>
    
</section>