<?js
    var data = obj;
    var self = this;
?>
<section data-file="members.tmpl" class="">
    <h4 class="name" id="<?js= id ?>">
        <?js= data.attribs + name + (data.signature ? data.signature : '') ?>
        <span class="uk-label">Member</span>
    </h4>

    <!-- <?js if (data.summary) { ?>
        <div class="summary">
            <?js= summary ?>
        </div>
    <?js } ?> -->

    <?js= this.partial('details.tmpl', data) ?>

    <?js if (data.description) { ?>
        <div class="description uk-margin-small">
            <?js= data.description ?>
        </div>
    <?js } ?>

    <?js if (data.type && data.type.names) {?>
        <h5>Type:</h5>
        <ul>
            <li>
                <?js= self.partial('type.tmpl', data.type.names) ?>
            </li>
        </ul>
    <?js } ?>

    <?js if (data.fires && fires.length) { ?>
        <h5>Fires:</h5>
        <ul><?js fires.forEach(function(f) { ?>
            <li><?js= self.linkto(f) ?></li>
        <?js }); ?></ul>
    <?js } ?>

    <?js if (data.examples && examples.length) { ?>
        <h5 class="uk-margin-small-bottom">Example<?js= examples.length > 1? 's':'' ?></h5>
        <?js= this.partial('examples.tmpl', examples) ?>
    <?js } ?>
</section>